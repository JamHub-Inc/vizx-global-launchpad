{
  "version": 2,
  "builds": [
    {
      "src": "api/chat/route.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/chat",
      "methods": ["POST", "OPTIONS"],
      "dest": "/api/chat/route.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}